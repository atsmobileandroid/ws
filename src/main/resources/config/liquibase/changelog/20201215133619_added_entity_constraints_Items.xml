<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Items.
    -->
    <changeSet id="20201215133619-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="item_prices_id"
                                 baseTableName="items"
                                 constraintName="fk_items_item_prices_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item_prices"/>

        <addForeignKeyConstraint baseColumnNames="basket_id"
                                 baseTableName="items"
                                 constraintName="fk_items_basket_id"
                                 referencedColumnNames="id"
                                 referencedTableName="basket"/>

    </changeSet>
</databaseChangeLog>
