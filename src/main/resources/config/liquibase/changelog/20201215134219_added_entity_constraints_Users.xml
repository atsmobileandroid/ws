<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Users.
    -->
    <changeSet id="20201215134219-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="basket_id"
                                 baseTableName="users"
                                 constraintName="fk_users_basket_id"
                                 referencedColumnNames="id"
                                 referencedTableName="basket"/>

        <addForeignKeyConstraint baseColumnNames="countries_id"
                                 baseTableName="users"
                                 constraintName="fk_users_countries_id"
                                 referencedColumnNames="id"
                                 referencedTableName="countries"/>

        <addForeignKeyConstraint baseColumnNames="user_type_id"
                                 baseTableName="users"
                                 constraintName="fk_users_user_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_type"/>

    </changeSet>
</databaseChangeLog>
